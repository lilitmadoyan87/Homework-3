function count(arr) {
  let countInt = 0;
  let countStr = 0;
  for(let i = 0; i < arr.length; i++) {
    if(typeof arr[i] === 'number') {
      countInt++
    } else if(typeof arr[i] === 'string') {
      countStr++
    }
  }  
  return `Numbers: ${countInt}, Strings: ${countStr}`;
}

 let arr = [1, 2, 3, 'Hello', 'world', 7, 4, 'Republic of Artsakh'];
 console.log(count(arr));
